machttest1 <- function(N, beta0, betaA, reps, alpha) {
res <- vector('numeric', reps)
for (i in 1:reps){
X_sim <- sim(N, betaA)
p.val <- test(X_sim, beta0)
res[i] <- (p.val < alpha)
}
list(m=mean(res), s=sd(res)/sqrt(reps))
}
